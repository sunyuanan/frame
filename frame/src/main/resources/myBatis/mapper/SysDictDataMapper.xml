    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, DICT_SORT, DICT_LABEL, DICT_VALUE, DICT_TYPE, STATUS, CREATE_TIME, UPDATE_TIME, SYNC_TIME, REMARK, TYPE_ID
