<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.frame.mapper.SysInterceptLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.frame.entity.SysInterceptLog">
        <id column="id" property="id"/>
        <result column="ip" property="ip"/>
        <result column="request_url" property="requestUrl"/>
        <result column="method" property="method"/>
        <result column="params" property="params"/>
        <result column="create_time" property="createTime"/>
        <result column="sync_time" property="syncTime"/>
        <result column="error_info" property="errorInfo"/>


    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ip, request_url, method, params, create_time, sync_time,error_info
    </sql>



</mapper>
